        "Catesta",
        "CICD",
        "cmdlets",
        "distros",
        "dotnet",
        "drawio",
        "Evalz",
        "fullname",
        "githubactions",
        "gitignore",
        "GNULICENSE",
        "hashtable",
        "hhmmss",
        "ISCLICENSE",
        "jakemorrison",
        "jaketest",
        "json",
        "MITLICENSE",
        "moduleonly",
        "multichoice",
        "NOCHANGELOG",
        "NOCONDUCT",
        "NOCONTRIBUTING",
        "NOGITHUB",
        "notmatch",
        "NOTVAULT",
        "Nunit",
        "OTBS",
        "outputfile",
        "pkgs",
        "plastertest",
        "ps1xml",
        "pullrequest",
        "pwsh",
        "pwshcore",
        "Stroustrup",
        "vscode",
        "wontfix",
        "yaml",
        "yml"
